tryte  sugar  0h5F9
word   milk   0t210
triple egg    4
const  flour  0bN10N1

Beginning
    milk ~|= flour
    egg = flour * ~sugar
    store 0hAQG milk
Start
    goto egg ? flour
        <  Increase
        == End
# This is an important section.
Decrease
    1a = sugar
    1b = milk 
    call 3
    egg -/= 1a
    goto Start
# Only in-line
# comments.
Increase
    9c = flour ? milk >= 2 : flour
    call 5
    egg++
    goto Start